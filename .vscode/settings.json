{
  "nuxtr.monorepoMode.DirectoryName": "./frontend",
  "nuxtr.openItemsAfterCreation": true,
  "nuxtr.defaultPackageManager": "pnpm",
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "tailwindCSS.classAttributes": [
    "class",
    "ui"
  ],
  "tailwindCSS.experimental.classRegex": [
    [
      "ui:\\s*{([^)]*)\\s*}",
      "(?:'|\"|`)([^']*)(?:'|\"|`)"
    ]
  ]
}